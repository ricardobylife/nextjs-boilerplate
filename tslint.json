{
  "extends": [
    "tslint-config-prettier",
    "tslint-react"
  ],
  "rules": {
    "ban": false,
    "class-name": false,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "eofline": false,
    "forin": true,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsx-wrap-multiline": false,
    "jsdoc-format": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false,
    "label-position": true,
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [
      false,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "no-parameter-reassignment": false,
    "radix": true,
    "switch-default": true,
    "trailing-comma": [
      false
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "variable-name": false
  }
}
